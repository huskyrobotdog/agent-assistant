{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AgentAssistant",
  "version": "1.0.0",
  "identifier": "com.husky.agent-assistant",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Agent助手",
        "width": 1024,
        "height": 768,
        "minWidth": 1024,
        "minHeight": 768,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "dmg",
      "nsis"
    ],
    "macOS": {
      "minimumSystemVersion": "11.0"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/agent"
    ]
  }
}